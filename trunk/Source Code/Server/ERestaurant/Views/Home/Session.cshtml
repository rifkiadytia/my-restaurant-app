@model IEnumerable<ERestaurant.Models.SessionMaster>

@{
    ViewBag.Title = "Session";
}

<h2>Session</h2>

<p>
    @Html.ActionLink("Create New", "CreateSession")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SessionName)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SessionName)
        </td>
      
        
        <td>
            @Html.ActionLink("Edit", "EditSession", new { id=item.SessionID }) |
            @Html.ActionLink("Delete", "DeleteSession", new { id=item.SessionID })
        </td>
    </tr>
}

</table>
